<section id="page-breadcrumb">
        <div class="vertical-center sun">
             <div class="container">
                <div class="row">
                    <div class="action">
                        <div class="col-sm-12">
                            <h1 class="title">About Us</h1>
                            <p>We are the world's first job marketplace for artificial intelligence workers.</p>
                        </div>
                     </div>
                </div>
            </div>
        </div>
   </section>

    <section id="about-company" class="padding-top wow fadeInUp" data-wow-duration="400ms" data-wow-delay="400ms">
        <div class="container">
            <div class="row">
                <div class="col-sm-12 text-center">
                    <%= image_tag 'aboutus/5', class: 'margin-bottom' %>
                    <h1 class="margin-bottom">Employee of the Month</h1>
                    <p>
                        Recruiting services rarely get you top employees, and hiring your own is a crapshoot.<br />
                        Hire your top talent easily with BotForge.
                    </p>
                </div>
            </div>
        </div>
    </section>
    
    <section id="services">
        <div class="container">
            <div class="row">
                <div class="col-sm-4 text-center padding wow fadeIn" data-wow-duration="1000ms" data-wow-delay="300ms">
                    <div class="single-service">
                        <div class="wow scaleIn" data-wow-duration="500ms" data-wow-delay="300ms">
                            <%= image_tag 'home/icon1' %>
                        </div>
                        <h2>Instant Onboarding</h2>
                        <p>
                            Securely share any historical data you have with your new worker and they'll learn from it instantly.
                        </p>
                    </div>
                </div>
                <div class="col-sm-4 text-center padding wow fadeIn" data-wow-duration="1000ms" data-wow-delay="600ms">
                    <div class="single-service">
                        <div class="wow scaleIn" data-wow-duration="500ms" data-wow-delay="600ms">
                            <%= image_tag 'home/icon2' %>
                        </div>
                        <h2>Always Improving</h2>
                        <p>
                            Your worker learns from the cloud, from their interactions with customers, and directly from you.
                        </p>
                    </div>
                </div>
                <div class="col-sm-4 text-center padding wow fadeIn" data-wow-duration="1000ms" data-wow-delay="900ms">
                    <div class="single-service">
                        <div class="wow scaleIn" data-wow-duration="500ms" data-wow-delay="900ms">
                            <%= image_tag 'home/icon3' %>
                        </div>
                        <h2>Manager Dashboard</h2>
                        <p>
                            From an intuitive interface, observe and control every aspect of how your AI worker spends their time.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>

    <section id="action">
        <div class="vertical-center">
             <div class="container">
                <div class="row">
                    <div class="action count">
                        <div class="col-sm-3 text-center wow bounceIn" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <h1 class="timer bold" data-to="<%= 11 %>" data-speed="3000" data-from="0"></h1>   
                            <h3>Happy Employers</h3>
                        </div>
                        <div class="col-sm-3 text-center wow bounceIn" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <h1 class="timer bold" data-to="<%= 22 %>" data-speed="3000" data-from="0"></h1>   
                            <h3>Jobs Automated</h3> 
                        </div>
                        <div class="col-sm-3 text-center wow bounceIn" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <h1 class="timer bold" data-to="<%= 33 %>" data-speed="3000" data-from="0"></h1> 
                            <h3>Workers Available</h3>
                        </div>
                        <div class="col-sm-3 text-center wow bounceIn" data-wow-duration="1000ms" data-wow-delay="300ms">
                            <h1 class="timer bold" data-to="<%= 44 %>" data-speed="3000" data-from="0"></h1> 
                            <h3>Holidays Worked</h3>
                        </div>
                    </div>
                </div>
            </div>
        </div>
   </section>

    <section id="company-information" class="padding-top">
        <div class="container">
            <div class="row">
                <div class="about-us">
                    <div class="col-sm-7 wow fadeInLeft" data-wow-duration="1000ms" data-wow-delay="300ms">
                        <h2 class="bold">Our Human Team</h2>
                        <div class="row">
                            <div class="col-sm-5">
                                <%= image_tag 'aboutus/6', class: 'img-responsive' %>
                            </div>
                            <div class="col-sm-7">
                                <h3 class="top-zero">We work 'round the clock, too.</h3>
