                            <div class="single-skill">
                                <h3>Automated Task Specialists</h3>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-primary six-sec-ease-in-out" role="progressbar"  data-transition="90">123</div>
                                </div>
                            </div>
                            <div class="single-skill">
                                <h3>Information Analysts</h3>
                                <div class="progress">
                                    <div class="progress-bar progress-bar-primary six-sec-ease-in-out" role="progressbar"  data-transition="45">45</div>
